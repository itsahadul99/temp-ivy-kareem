<style>
  #Veneto_Living_Chair_section {
    padding: 100px 110px;
    margin: 0 40px;
    background: #252531;
  }
  .Veneto_Living_Chair_section_wrapper {
    margin: 0 auto;
    max-width: 1440px;
    display: flex;
    align-items: center;
  }

  .Veneto_Living_Chair_section_wrapper .left-side {
    max-width: 22%;
    min-width: 22%;
    padding: 0 20px 0 0;
    max-height: 255px;
    overflow-y: auto;
  }

  /* Common scrollbar styles */
  .Veneto_Living_Chair_section_wrapper .left-side::-webkit-scrollbar {
    width: 6px;
    height: 6px;
  }

  /* Track styles for both horizontal and vertical */
  .Veneto_Living_Chair_section_wrapper .left-side::-webkit-scrollbar-track {
    background: #f1f1f1;
    border-radius: 10px;
  }

  /* Thumb styles for both horizontal and vertical */
  .Veneto_Living_Chair_section_wrapper .left-side::-webkit-scrollbar-thumb {
    background: #6ca082;
    border-radius: 10px;
  }

  /* Thumb hover effect */
  .Veneto_Living_Chair_section_wrapper .left-side::-webkit-scrollbar-thumb:hover {
    background: #6ca082aa;
  }

  .Veneto_Living_Chair_section_wrapper .left-side::-webkit-scrollbar:horizontal {
    height: 6px;
  }

  .Veneto_Living_Chair_section_wrapper .left-side::-webkit-scrollbar:vertical {
    width: 6px;
  }


  .Veneto_Living_Chair_section_wrapper .left-side li {
    color: #fff;
    font-size: 20px;
    font-style: normal;
    font-weight: 500;
    line-height: normal;
    display: flex;
    align-items: center;
    justify-content: space-between;
    cursor: pointer;
  }
  .Veneto_Living_Chair_section_wrapper .left-side li svg {
    fill: #ffffff;
  }

  .Veneto_Living_Chair_section_wrapper .left-side li.active {
    color: var(--6-ca-082, #6ca082);
  }

  .Veneto_Living_Chair_section_wrapper .left-side li.active svg {
    fill: #6ca082 !important;
  }

  .Veneto_Living_Chair_section_wrapper .right-side {
    display: inline-flex;
    padding: 20px 10.18px 40px 40px;
    justify-content: flex-end;
    align-items: flex-start;
    gap: 16.09px;
  }

  .Veneto_Living_Chair_section_wrapper .right-side div {
    display: flex;
    flex-direction: column;
    align-items: flex-start;
    gap: 40px;
  }
  .Veneto_Living_Chair_section_wrapper .right-side div h2 {
    color: #fff;

    font-size: 32px;
    font-style: normal;
    font-weight: 500;
    line-height: normal;
  }
  .Veneto_Living_Chair_section_wrapper .right-side div p {
    color: rgba(255, 255, 255, 0.6);
    font-family: Avenir LT Pro;
    font-size: 16px;
    font-style: normal;
    font-weight: 400;
    line-height: 150%;
    /* 24px */
  }
  .Veneto_Living_Chair_section_wrapper .right-side img {
    max-width: 270px;
    min-width: 270px;
    max-height: 270px;
    min-height: 270px;
    border-radius: 100%;
  }

  .marquee {
    overflow: hidden;
    display: flex;
    gap: 40px;
  }

  @media (max-width: 768px) {
    #Veneto_Living_Chair_section {
      padding: 48px 32px;
      margin: 0;
    }
    .Veneto_Living_Chair_section_wrapper {
      flex-direction: column;
    }

    .Veneto_Living_Chair_section_wrapper .left-side {
      max-width: 100%;
      min-width: 100%;
      padding: 0 0 20px;
      border-right: none;
      border-bottom: 2px solid rgba(255, 255, 255, 0.1);
      display: flex;
      justify-content: space-between;
      gap: 30px;
      overflow-x: auto;
    }
    .Veneto_Living_Chair_section_wrapper .left-side li {
      gap: 24px;
    }

    .Veneto_Living_Chair_section_wrapper .right-side {
      display: inline-flex;
      padding: 40px 0 48px;
      justify-content: flex-end;
      align-items: flex-start;
      gap: 40px;
    }
    .Veneto_Living_Chair_section_wrapper .right-side img {
      max-width: 181px;
      min-width: 181px;
      max-height: 181px;
      min-height: 181px;
      aspect-ratio: 1 / 1;
    }

    .Veneto_Living_Chair_section_wrapper .right-side div h2 {
      font-size: 40px;
    }
    .Veneto_Living_Chair_section_wrapper .right-side div p {
      font-size: 14px;
      line-height: 21px;
    }

  }

  @media (max-width: 426px) {
    #Veneto_Living_Chair_section {
      padding: 32px 20px;
    }

    .Veneto_Living_Chair_section_wrapper .left-side {
      padding: 0 10px 0 0;
      gap: 8px;
      overflow: auto;
    }
    .Veneto_Living_Chair_section_wrapper .left-side li {
      font-weight: 600;

      gap: 16px;
    }
    .Veneto_Living_Chair_section_wrapper .right-side {
      padding: 32 0 0;
      flex-direction: column;
      justify-content: center;
      align-items: center;
      gap: 32px;
    }

    .Veneto_Living_Chair_section_wrapper .right-side img {
      max-width: 280px;
      min-width: 280px;
      max-height: 280px;
      min-height: 280px;
    }

    .Veneto_Living_Chair_section_wrapper .right-side div {
      gap: 12px;
    }
    .Veneto_Living_Chair_section_wrapper .right-side div p {
      font-size: 16px;
    }
  }
</style>

<section id="Veneto_Living_Chair_section">
  <div class="Veneto_Living_Chair_section_wrapper">
    <ul class="left-side">
      {% for block in section.blocks %}
        <li class="{% if forloop.first %}active{% endif %}" data-index="{{ forloop.index0 }}">
          <p>{{ block.settings.left_title }}</p>
          {{ block.settings.left_icon }}
        </li>
      {% endfor %}
    </ul>

    <div class="right-side">
      {% if section.blocks.size > 0 %}
        {% assign first_block = section.blocks.first %}
        <img
          src="{{ first_block.settings.right_image | img_url: '270x270' }}"
          alt="{{ first_block.settings.right_title }}"
          id="right-image">
        <div>
          <h2 id="right-title">{{ first_block.settings.right_title }}</h2>
          <p id="right-description">{{ first_block.settings.right_description }}</p>
        </div>
      {% endif %}
    </div>
  </div>
</section>

<script>
  document.addEventListener('DOMContentLoaded', function() {
    const categories = [
      {% for block in section.blocks %}
        {
          image: "{{ block.settings.right_image | img_url: '270x270' }}",
          title: {{ block.settings.right_title | json }},
          description: {{ block.settings.right_description | json }}
        }{% unless forloop.last %},{% endunless %}
      {% endfor %}
    ];
  
    const listItems = document.querySelectorAll('.left-side li');
    const rightImage = document.getElementById('right-image');
    const rightTitle = document.getElementById('right-title');
    const rightDescription = document.getElementById('right-description');
  
    function updateContent(index) {
      listItems.forEach(li => li.classList.remove('active'));
      listItems[index].classList.add('active');
      rightImage.src = categories[index].image;
      rightTitle.textContent = categories[index].title;
      rightDescription.textContent = categories[index].description;
    }
  
    listItems.forEach((item, index) => {
      item.addEventListener('click', () => updateContent(index));
    });
  });
</script>

