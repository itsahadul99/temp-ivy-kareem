      </div>
    </div>

    <script>
      function playVideo(videoId) {
        const video = document.getElementById(videoId); 
        const defaultVideo = document.querySelector('.default');

        defaultVideo.classList.remove('active'); 
        video.classList.add('active'); 
        video.play(); 
      }

   
      function pauseVideo(videoId) {
        const video = document.getElementById(videoId); 
        const defaultVideo = document.querySelector('.default'); 

        video.classList.remove('active');
        defaultVideo.classList.add('active'); 
        video.pause(); 
        video.currentTime = 0; 
      }

      document.addEventListener('DOMContentLoaded', function () {
        const carousel = document.getElementById('carousel');
        const slides = document.querySelectorAll('.slide');
        const visibleSlides = 3;
        const gap = 50; // Gap in pixels
        const slideWidth = slides[0].offsetWidth + gap;
        let currentIndex = 0;
        let isTransitioning = false;

   
        slides.forEach((slide) => {
          let clone = slide.cloneNode(true);
          carousel.appendChild(clone);
        });

        function moveCarousel() {
          if (isTransitioning) return;
          isTransitioning = true;

          currentIndex++;
          carousel.style.transition = 'transform 1s ease-in-out';
          carousel.style.transform = `translateX(${-currentIndex * slideWidth}px)`;

          setTimeout(() => {
            if (currentIndex >= slides.length) {
              carousel.style.transition = 'none';
              currentIndex = 0;
              carousel.style.transform = `translateX(${0}px)`;
            }
            isTransitioning = false;
          }, 1000); 
        }

        setInterval(moveCarousel, 3000);
      });

      const carousel = document.getElementById('carousel');
      const slides = carousel.children;
      const totalSlides = slides.length;
      const visibleSlides = 3;
      const gap = 50; 
      const slideWidth = slides[0].offsetWidth + gap;

  
      for (let i = 0; i < visibleSlides; i++) {
        const cloneFirst = slides[i].cloneNode(true); 
        const cloneLast = slides[totalSlides - 1 - i].cloneNode(true);
        carousel.appendChild(cloneFirst); 
        carousel.prepend(cloneLast); 
      }

      let currentIndex = visibleSlides; 
      carousel.style.transform = `translateX(${-currentIndex * slideWidth}px)`;

 
      function moveCarousel(direction) {
        currentIndex += direction;

        carousel.style.transition = 'transform 1s ease-in-out';
        carousel.style.transform = `translateX(${-currentIndex * slideWidth}px)`;

        
        setTimeout(() => {
          if (currentIndex >= totalSlides + visibleSlides) {
         
            currentIndex = visibleSlides;
            carousel.style.transition = 'none';
            carousel.style.transform = `translateX(${-currentIndex * slideWidth}px)`;
          } else if (currentIndex < visibleSlides) {
     
            currentIndex = totalSlides + visibleSlides - 1;
            carousel.style.transition = 'none';
            carousel.style.transform = `translateX(${-currentIndex * slideWidth}px)`;
          }
        }, 1000); 
      }


      setInterval(() => moveCarousel(1), 3000);

      document.getElementById('menu-btn').addEventListener('click', function () {
        let menu = document.getElementById('menu');
        menu.classList.toggle('hidden');
      });

      document.addEventListener('DOMContentLoaded', function () {
        const menu = document.getElementById('menu');
        const menuBtn = document.getElementById('menu-btn');
        const closeBtn = document.getElementById('close-btn');

        function showMenu() {
          menu.classList.remove('hidden');
          setTimeout(() => {
            menu.classList.remove('opacity-0', 'scale-95');
          }, 10);
        }

        function hideMenu() {
          menu.classList.add('opacity-0', 'scale-95');
          setTimeout(() => {
            menu.classList.add('hidden');
          }, 500);
        }

        menuBtn.addEventListener('click', showMenu);
        closeBtn.addEventListener('click', hideMenu);
      });
    </script>
