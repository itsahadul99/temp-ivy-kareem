        #carousel {
          display: flex;
          gap: 50px;
          animation: scroll 15s linear infinite; 
        }

        .slide {
          flex-shrink: 0;
          width: calc(100% / 3); 
        }

        .slide img {
          width: 100%;
          height: 337px; 
          object-fit: cover;
        }

    
        @keyframes scroll {
          from {
            transform: translateX(0);
          }
          to {
            transform: translateX(-100%); 
          }
